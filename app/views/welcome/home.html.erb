            <td style="text-align: center;"><a ng-href='{{plan.monUrl}}' target="_blank">{{plan.monData | number:2}}</a></td>
            <td style="text-align: center;"><a ng-href='{{plan.tuesUrl}}' target="_blank">{{plan.tuesData | number:2}}</a></td>
            <td style="text-align: center;"><a ng-href='{{plan.wedUrl}}' target="_blank">{{plan.wedData | number:2}}</a></td>
            <td style="text-align: center;"><a ng-href='{{plan.thursUrl}}' target="_blank">{{plan.thursData | number:2}}</a></td>
